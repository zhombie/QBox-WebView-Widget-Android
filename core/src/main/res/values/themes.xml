<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.WebViewWidget" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
    </style>

    <style name="QboxWidget.Toolbar.TitleTextAppearance" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="QboxWidget.Toolbar.SubTitleTextAppearance" parent="TextAppearance.Widget.AppCompat.Toolbar.Subtitle">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="QboxWidget.Toolbar" parent="Widget.AppCompat.Toolbar">
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
        <item name="titleTextAppearance">@style/QboxWidget.Toolbar.TitleTextAppearance</item>
        <item name="subtitleTextAppearance">@style/QboxWidget.Toolbar.SubTitleTextAppearance</item>
    </style>

</resources>